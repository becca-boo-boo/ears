{
  "id" : "f101",
  "orgId" : "comcast",
  "appId" : "xfi",
  "userId" : "boris",
  "name" : "simpleFilterLongChainRoute",
  "receiver" : {
    "plugin" : "debug",
    "name" : "simpleFilterLongChainRouteReceiver",
    "config" :
    {
      "rounds" : 5,
      "intervalMs" : 10,
      "payload" : {
        "a": {
          "b": {
            "c": [
              {
                "foo": "bar"
              },
              {
                "foo": "bar"
              }
            ]
          }
        }
      },
      "maxHistory": 100
    }
  },
  "sender" : {
    "plugin" : "debug",
    "name" : "simpleFilterLongChainRouteSender",
    "config" : {
      "destination" : "stdout",
      "maxHistory": 100
    }
  },
  "filterChain" : [
    {
      "plugin" : "unwrap",
      "name" : "simpleFilterLongChainRouteUnwrapper",
      "config" : {
        "unwrapPath" : "a.b.c"
      }
    },
    {
      "plugin" : "split",
      "name" : "simpleFilterLongChainRouteSplitter",
      "config" : {
        "splitPath" : "."
      }
    },
    {
      "plugin" : "match",
      "name" : "simpleFilterLongChainRouteMatchAllowA",
      "config" : {
        "mode" : "allow",
        "matcher" : "regex",
        "pattern" : "^.*$"
      }
    },
    {
      "plugin" : "match",
      "name" : "simpleFilterLongChainRouteMatchAllowB",
      "config" : {
        "mode" : "allow",
        "matcher" : "regex",
        "pattern" : "^.*$"
      }
    },
    {
      "plugin" : "match",
      "name" : "simpleFilterLongChainRouteMatchAllowC",
      "config" : {
        "mode" : "allow",
        "matcher" : "regex",
        "pattern" : "^.*$"
      }
    }
  ],
  "deliveryMode" : "whoCares"
}
