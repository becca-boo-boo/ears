  },
  "item": {
    "deliveryMode": "whoCares",
    "filterChain": [
      {
        "config": {
          "matcher": "patternregex",
          "mode": "allow",
          "pattern": {
            "TopicArn": ".*_ACCOUNT$|.*_ACCOUNTPRODUCT$"
          }
        },
        "name": "useCaseOneRouteRegexMatcher",
        "plugin": "match"
      },
      {
        "config": {
          "fromPath": ".Message"
        },
        "name": "useCaseOneRouteDecoder",
        "plugin": "decode"
      },
      {
        "config": {
          "matcher": "pattern",
          "mode": "allow",
